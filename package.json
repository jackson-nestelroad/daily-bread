{
  "name": "daily-bread",
  "type": "module",
  "version": "1.4.0",
  "description": "TypeScript library for reading the Bible.",
  "author": "jackson-nestelroad",
  "license": "ISC",
  "repository": "github:jackson-nestelroad/daily-bread",
  "keywords": [
    "bible",
    "scripture",
    "christian"
  ],
  "files": [
    "build/cli/**/*.js",
    "build/cli/**/*.js.map",
    "build/cli/**/*.d.ts",
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "!build/**/*.spec.*"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "bin": "build/cli/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node build/src/index.js",
    "test": "npm run build && mocha --parallel --recursive build/src",
    "cli": "npm run build && node build/cli/index.js",
    "dryrun": "npm pack --dry-run"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.0.2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "@types/sinon": "^10.0.13",
    "@types/yargs": "^17.0.24",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.6",
    "sinon": "^15.0.3"
  }
}
